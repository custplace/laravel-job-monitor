{
    "name": "custplace/laravel-job-monitor",
    "description": "A Laravel package to monitor job execution time and notify about stuck jobs",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0",
        "illuminate/database": "^9.0|^10.0",
        "illuminate/queue": "^9.0|^10.0",
        "illuminate/console": "^9.0|^10.0",
        "illuminate/notifications": "^9.0|^10.0",
        "custplace/laravel-job-monitor": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Custplace\\LaravelJobMonitor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Custplace\\JobMonitor\\JobMonitorServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/custplace/laravel-job-monitor.git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
